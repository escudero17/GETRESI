/* Estilo para el modal */
.modal {
    display: none; /* Inicialmente oculto */
    position: fixed; /* Posicionamiento fijo */
    z-index: 1; /* Prioridad */
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    overflow: auto; /* Permite el scroll si el contenido es demasiado grande */
    background-color: rgba(0, 0, 0, 0.4); /* Fondo oscuro con transparencia */
}

/* Contenido del modal */
.modal-content {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background-color: white;
    color: #D5254B;
    margin: 15% auto; /* Márgenes arriba y abajo, centrado horizontalmente */
    padding: 20px;
    border: 2px solid #D5254B;
    border-radius: 20px;
    width: 30%; /* Ancho del modal */
    height: 30%;
}
.modal-content>a{
    display: flex;
    align-items: center;
    justify-content: center;
    text-decoration: none;
    width: 250px;
    height: 40px;
    border: none;
    border-radius: 10px;
    font-size: 15px;
    color: white;
    cursor: pointer;
    background-color: #D5254B;
}

#modalAyuda input{
 border: #D5254B solid 2px;
 border-radius: 3px;

 margin: 10px;
}
#correo{
width:300px;
height: 20px;
}
#comentario{
width: 300px;
height: 70px;

}
/* Botón para cerrar el modal */
.close {
    position: absolute;
    bottom: 440px;
    right: 550px;
    color: black;
    float: right;
    font-size: 28px;
    font-weight: bold;
    cursor: pointer;
}

.close:hover,
.close:focus {
    color: black;
    text-decoration: none;
    cursor: pointer;
}

